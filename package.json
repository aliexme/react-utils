{
  "name": "@aliexme/react-utils",
  "version": "0.1.0",
  "description": "React utilites",
  "private": false,
  "author": "Alexander Smirnov <al.smirnov996@gmail.com>",
  "homepage": "https://github.com/aliexme/js-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliexme/js-utils"
  },
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install && yarn ts:compile",
    "ts:check": "tsc --noEmit",
    "ts:compile": "tsc -p ./tsconfig.lib.json",
    "lint:check": "eslint --ext .js,.mjs,.ts,.tsx --ignore-path .gitignore --cache ./",
    "lint:fix": "yarn lint:check --fix",
    "test": "jest",
    "test:coverage": "jest --collectCoverage"
  },
  "devDependencies": {
    "@aliexme/eslint-config": "0.2.7",
    "@aliexme/ts-types": "0.2.3",
    "@types/jest": "29.5.0",
    "@types/react": "18.0.31",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
